{
	"schemaVersion": 1,
	"id": "mcai",
	"version": "1",
	"name": "Minecraft AI",
	"description": "This mod connects Minecraft with https://c.ai, allowing you to interact with AI inside of the game!",
	"authors": [
		"Alfred uwu"
	],
	"contact": {
		"homepage": ""
	},
	"license": "MIT",
	"icon": "assets/mcai/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			"com.alfred.ai.MCAIMod"
		],
		"modmenu": [
			"com.alfred.ai.compat.ModMenuIntegration"
		]
	},
	"mixins": [
